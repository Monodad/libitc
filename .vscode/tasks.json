{
	"version": "2.0.0",
	"presentation": {
		"echo": false,
		"reveal": "always",
		"focus": false,
		"panel": "shared",
		"showReuseMessage": true,
		"clear": true
	},
	"tasks": [
		{
			"label": "Build",
			"type": "shell",
			"command": [
				"set -o pipefail;",
				"rm libitc.qsf;",
				"(",
				"quartus_sh -t tools/assign_chenghsuan.tcl &&",
				"quartus_sh -t tools/compile.tcl",
				") | sed -e 's/^\\s*//g' -e '/^Info/d'"
			],
			"problemMatcher": [],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Deploy",
			"type": "shell",
			"command": [
				"set -o pipefail;",
				"rm libitc.qsf;",
				"(",
				"quartus_sh -t tools/assign_chenghsuan.tcl &&",
				"quartus_sh -t tools/compile.tcl &&",
				"quartus_pgm -c 'USB-Blaster [USB-0]' output_files/libitc.cdf",
				") | sed -e 's/^\\s*//g' -e '/^Info/d'"
			],
			"problemMatcher": [],
			"group": {
				"kind": "test",
				"isDefault": true
			}
		}
	]
}
