{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build",
			"type": "shell",
			"command": [
                "set -o pipefail",
				"quartus_sh -t tools/assign.tcl | sed -e 's/^\\s*//g' -e '/^Info/d' &&",
				"quartus_sh -t tools/compile.tcl | sed -e 's/^\\s*//g' -e '/^Info/d'",
			],
			"problemMatcher": [],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Deploy",
			"type": "shell",
			"command": [
                "set -o pipefail",
				"quartus_sh -t tools/assign.tcl | sed -e 's/^\\s*//g' -e '/^Info/d' &&",
				"quartus_sh -t tools/compile.tcl | sed -e 's/^\\s*//g' -e '/^Info/d' &&",
				"quartus_pgm -c 'USB-Blaster [USB-0]' output_files/libitc.cdf | sed -e 's/^\\s*//g' -e '/^Info/d'",
			],
			"problemMatcher": [],
			"group": {
				"kind": "test",
				"isDefault": true
			}
		}
	]
}
